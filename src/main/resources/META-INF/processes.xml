<?xml version="1.0" encoding="UTF-8" ?>
<process-application
        xmlns="http://www.camunda.org/schema/1.0/ProcessApplication"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <process-archive name="helloworld">
        <process-engine>default</process-engine>
        <resource>helloworld.bpmn</resource>
        <resource>tracking-process.bpmn</resource>
        <resource>ticketing-process.bpmn</resource>
        <resource>incident-process.bpmn</resource>
        <resource>complaints-process.bpmn</resource>
        <properties>
            <property name="isDeleteUponUndeploy">false</property>
            <property name="isScanForProcessDefinitions">true</property>
        </properties>
    </process-archive>


</process-application>

